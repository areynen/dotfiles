super + Return 
    $TERMINAL
super + { ,shift} + z 
    $BROWSER { ,--private-window}
super + alt + { ,shift} + z
    chromium { ,--incognito}
super + control + z
    searchcopied.sh
super + alt + q
    xkill
super + shift + alt + q
    kill_current
super + g
    gimp
super + w
    jdownloader
super + t
    thunar 
super + d
    dmenu_run
super + v
    clipmenu -i -l 20
super + c
    consume.sh -s -d -f $HOME/Documents/scripts/to_consume.txt
super + shift + c
    consume.sh -f $HOME/Documents/scripts/to_consume_long.txt
super + shift + t
    $TERMINAL -c "ranger" -e ranger
super + q 
    $TERMINAL -c "htop" -e htop
super + {_,shift} + m
    {$TERMINAL -c "neomutt" -e neomutt, updatemail.sh}
super + {_,shift} + u
    {$TERMINAL -c "ncmpcpp" -e ncmpcpp,ario}
super + p
    passmenu-otp
super + {_,shift} + n 
    {$TERMINAL -c "newsboat" -e newsboat && blocks_update 3, updatenews.sh}
super + { ,shift,alt} + o
    {mono.sh, dual-offset-home.sh, trio-offset-home.sh}
super + {_,shift,control,alt} +  s
    {screenshot_selected,screenshot_full,ocr,openlatest}  
super + shift + alt + s
    openlatest -s
super + alt + {Down,Left,Right}
    mpc {toggle,prev,next}; blocks_update 2
super + alt + shift + {Left,Right,Down}
    mpc volume {-5,+5,50}
super + alt + shift + Up
    mpcnotify.sh
super + alt + Up
    pavucontrol
{_,shift} + XF86Audio{Lower,Raise}Volume
    pactl set-sink-volume @DEFAULT_SINK@ {-5,-1,+5,+1}%
XF86AudioMute
    pactl set-sink-mute @DEFAULT_SINK@ toggle
{_,shift} + XF86Explorer
    {$FILES, $TERMINAL -c "ranger" -e ranger}
XF86Calculator
    galculator
XF86Mail
    $TERMINAL -c "neomutt" -e neomutt
{ ,shift} + XF86HomePage
    $BROWSER { ,--private-window}
super + {shift,control} + grave
    dunstctl {history-pop,close}
